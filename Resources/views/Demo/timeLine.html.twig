<div class="container">
    {% set errors = [400, 401, 402,403,404,405,406,407,408,409,410,411,412,413,414,415,416,417,500,501,502,503,504,505] %}
    {% for d in data %}
        <ul class="timeline">
            <li {% if loop.index%2 == 0 %} class="timeline-inverted" {% endif %}>
                {% if d.errorCode in errors%}
                    <div class="timeline-badge danger"><i class="glyphicon glyphicon-remove"></i></div>
                {% else %}
                    <div class="timeline-badge success"><i class="glyphicon glyphicon-ok"></i></div>
                {% endif %}

                <div class="timeline-panel">
                    <div class="timeline-heading">
                        <h4 class="timeline-title">{{ d.uri }}</h4>
                        <p><small class="text-muted"><i class="glyphicon glyphicon-time"></i> {{ d.date }}</small>
                                    {% if d.errorCode in errors%}
                                        <span class="label label-danger" style="margin-left: 20px;">{{ d.errorCode }}</span>
                                    {% else %}
                                        <span class="label label-success" style="margin-left: 20px;">{{ d.errorCode }}</span>
                                    {% endif %}
                        </p>
                    </div>
                </div>
            </li>
        </ul>
    {% endfor %}
</div>

